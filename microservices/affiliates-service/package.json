  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/app.js",
    "dev": "nodemon --exec \"ts-node -r tsconfig-paths/register src/app.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "affiliate",
    "mlm",
    "commissions",
    "cpa",
    "backend",
    "api",
    "typescript",
    "fastify"
