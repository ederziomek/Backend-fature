    "@fastify/swagger-ui": "^5.0.1",
    "@prisma/client": "^6.1.0",
    "axios": "^1.9.0",
    "dotenv": "^16.4.7",
    "fastify": "^5.1.0",
    "ioredis": "^5.4.1",
